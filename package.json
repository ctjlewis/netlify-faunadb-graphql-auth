{
  "name": "netlify-faunadb-graphql-auth",
  "version": "0.1.1",
  "description": "Netlify functions example with faunadb, graphql, and authorization",
  "scripts": {
    "create-db": "netlify dev:exec node scripts/create-database.js",
    "create-data": "netlify dev:exec node scripts/create-example-data.js",
    "bootstrap": "npx npm-run-all create-db create-data",
    "watch:src": "parcel --log-level 2 src/client/ui/index.html",
    "watch:server": "netlify-lambda serve src/server -c webpack.dev.js",
    "dev": "NODE_ENV=development npx npm-run-all -p watch:src watch:server",
    "build:server": "NODE_ENV=production netlify-lambda build src/server -c webpack.prod.js",
    "prebuild": "NODE_ENV=production npm run format && npm run build:server",
    "build": "NODE_ENV=production parcel build src/client/ui/index.html",
    "serve": "parcel serve src/client/ui/index.html",
    "format": "prettier \"{server,scripts,src}/**/*.{js,ts,tsx}\" --write",
    "lint": "eslint \"src/**/*.{js,jsx}\" --quiet"
  },
  "keywords": [
    "netlify",
    "faunadb",
    "graphql",
    "http-cookies"
  ],
  "author": "Paul Paterson",
  "license": "MIT",
  "dependencies": {
    "@apollo/client": "^3.3.2",
    "@apollo/link-context": "^2.0.0-beta.3",
    "apollo-link-context": "^1.0.20",
    "apollo-link-http": "^1.5.17",
    "apollo-server-lambda": "^2.19.2",
    "apollo-server-plugin-http-headers": "^0.1.4",
    "bufferutil": "^4.0.3",
    "cross-fetch": "^3.0.6",
    "draft-js": "^0.11.7",
    "faunadb": "^4.0.0",
    "graphql": "^15.4.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "utf-8-validate": "^5.0.4"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/preset-typescript": "^7.12.7",
    "@types/aws-lambda": "^8.10.71",
    "@types/node": "^14.14.22",
    "apollo-server-testing": "^2.19.2",
    "ava": "^3.15.0",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.4",
    "prettier": "^2.2.1",
    "request": "^2.88.2",
    "type-graphql": "^1.1.1",
    "typescript": "^4.1.3"
  }
}
